        "postcss-loader": "^5.3.0",
        "postcss-preset-env": "^6.7.0",
        "sass": "^1.32.13",
        "sass-loader": "^11.1.1",
        "style-loader": "^2.0.0",
        "terser-webpack-plugin": "^5.1.2",
